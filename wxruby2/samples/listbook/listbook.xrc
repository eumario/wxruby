                                                <item>small</item>
                                                <item>medium</item>
                                                <item>large</item>
                                                <item>x-large</item>
                                            </content>
                                            <selection>1</selection>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL|wxADJUST_MINSIZE</flag>
                                        <border>5</border>
