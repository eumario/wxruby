                                        <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxChoice" name="ID_PIZZA_SAUCE_CHOICE">
                                            <content>
                                                <item>red mild</item>
                                                <item>red spicy</item>
                                                <item>pesto</item>
                                                <item>creamy garlic</item>
