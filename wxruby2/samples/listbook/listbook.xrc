                                        <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxChoice" name="ID_PIZZA_TOPPING_3_CHOICE">
                                        </object>
                                    </object>
