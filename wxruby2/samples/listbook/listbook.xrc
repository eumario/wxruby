                                        <flag>wxALIGN_RIGHT|wxALIGN_BOTTOM|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxButton" name="ID_PIZZA_BUTTON">
                                            <label>Add to order</label>
                                        </object>
                                    </object>
