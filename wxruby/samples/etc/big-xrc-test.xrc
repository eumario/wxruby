<?xml version="1.0" encoding="ISO-8859-1"?>
<resource version="2.3.0.1" xmlns="http://www.wxwidgets.org/wxxrc">
  <object class="wxFrame" name="xrc_test_frame">
    <size>400,300</size>
    <title>XRC Test</title>
    <object class="wxPanel" name="content_panel">
      <style>wxTAB_TRAVERSAL</style>
      <object class="wxBoxSizer">
        <orient>wxVERTICAL</orient>
        <object class="sizeritem">
          <flag>wxGROW|wxALL</flag>
          <border>10</border>
          <option>1</option>
          <object class="wxNotebook" name="test_notebook">
          
            <!-- Static controls panel -->
            <object class="notebookpage">
              <label>Static controls</label>
              <object class="wxPanel" name="static_panel">
                <size>100,80</size>
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <option>1</option>
                    <object class="wxStaticBoxSizer" name="text_static_box">
                      <orient>wxVERTICAL</orient>
                      <label>Text and Bitmap</label>
                      <object class="spacer">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                        <border>5</border>
                        <option>1</option>
                        <size>5,5</size>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALL|wxADJUST_MINSIZE</flag>
                        <border>5</border>
                        <object class="wxStaticText" name="text_static">
                          <label>This is some static text.</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxStaticBitmap" name="static_bitmap">
                        </object>
                      </object>
                      <object class="spacer">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                        <border>5</border>
                        <option>1</option>
                        <size>5,5</size>
                      </object>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <option>1</option>
                    <object class="wxStaticBoxSizer" name="lines_static_box">
                      <orient>wxHORIZONTAL</orient>
                      <label>Lines</label>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <option>1</option>
                        <object class="wxStaticLine" name="line_h_static">
                          <style>wxLI_HORIZONTAL</style>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxALL</flag>
                        <border>5</border>
                        <object class="wxStaticLine" name="line_v_static">
                          <style>wxLI_VERTICAL</style>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            
            <!-- Buttons panel -->
            <object class="notebookpage">
              <label>Buttons</label>
              <object class="wxPanel" name="button_panel">
                <size>100,80</size>
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <object class="wxFlexGridSizer">
                      <cols>3</cols>
                      <rows>0</rows>
                      <vgap>0</vgap>
                      <hgap>0</hgap>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxButton" name="test_button">
                          <label>Button</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxBitmapButton" name="test_bitmap_button">
                          <style>wxBU_AUTODRAW|wxBU_EXACTFIT</style>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxToggleButton" name="test_toggle_button">
                          <label>Toggle</label>
                          <checked>0</checked>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxSpinButton" name="test_spin_button">
                          <style>wxSP_VERTICAL</style>
                          <value>0</value>
                          <min>0</min>
                          <max>100</max>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxRadioButton" name="test_radio_button">
                          <label>Radio Button</label>
                          <value>0</value>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxCheckBox" name="test_checkbox">
                          <label>Checkbox</label>
                          <checked>0</checked>
                        </object>
                      </object>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <object class="wxRadioBox" name="test_radiobox">
                      <size>-1,-1</size>
                      <style>wxRA_VERTICAL</style>
                      <label>Best Star Wars film</label>
                      <selection>1</selection>
                      <content>
                        <item>A New &amp;Hope</item>
                        <item>The &amp;Empire Strikes Back</item>
                        <item>&amp;Return of the Jedi</item>
                      </content>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            
            <!-- Text panel -->
            <object class="notebookpage">
              <label>Text</label>
              <object class="wxPanel" name="text_panel">
                <size>100,80</size>
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <option>1</option>
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <rows>0</rows>
                      <vgap>0</vgap>
                      <hgap>0</hgap>
                      <object class="sizeritem">
                        <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>10</border>
                        <object class="wxComboBox" name="test_combobox">
                          <style>wxCB_DROPDOWN</style>
                          <content>
                             <item>Apple</item>
                             <item>Orange</item>
                             <item>Grape</item>
                             <item>Lemon</item>
                          </content>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>10</border>
                        <object class="wxChoice" name="test_choice">
                          <content>
                            <item>Red</item>
                            <item>Orange</item>
                            <item>Yellow</item>
                            <item>Green</item>
                            <item>Blue</item>
                            <item>Violet</item>
                          </content>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>10</border>
                        <object class="wxTextCtrl" name="test_text">
                          <value>Text</value>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>10</border>
                        <object class="wxSpinCtrl" name="test_spin">
                          <style>wxSP_ARROW_KEYS</style>
                          <value>0</value>
                          <min>0</min>
                          <max>100</max>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxGROW|wxALL</flag>
                        <border>10</border>
                        <object class="wxListBox" name="test_listbox">
                          <style>wxLB_SINGLE</style>
                          <selection>1</selection>
                          <content>
                            <item>Peanut</item>
                            <item>Walnut</item>
                            <item>Cashew</item>
                            <item>Pecan</item>
                          </content>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxGROW|wxALL</flag>
                        <border>10</border>
                        <object class="wxListCtrl" name="test_list">
                          <size>100,100</size>
                          <style>wxLC_REPORT</style>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            
            <!-- Calendar panel -->
            <object class="notebookpage">
              <label>Calendar</label>
              <object class="wxPanel" name="calendar_panel">
                <size>100,80</size>
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <flag>wxALIGN_LEFT|wxALL</flag>
                    <border>15</border>
                    <object class="wxCalendarCtrl" name="test_calendar">
                      <size>200,150</size>
                      <style>wxCAL_SUNDAY_FIRST|wxCAL_SHOW_HOLIDAYS|wxCAL_SHOW_SURROUNDING_WEEKS</style>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            
            <!-- Misc panel -->
            <object class="notebookpage">
              <label>Misc</label>
              <object class="wxPanel" name="misc_panel">
                <size>100,80</size>
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>5</border>
                    <option>1</option>
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <rows>2</rows>
                      <vgap>0</vgap>
                      <hgap>0</hgap>
                      <object class="sizeritem">
                        <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxBoxSizer">
                          <orient>wxVERTICAL</orient>
                          <object class="sizeritem">
                            <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                            <border>5</border>
                            <object class="wxGauge" name="test_gauge">
                              <size>100,16</size>
                              <style>wxGA_HORIZONTAL</style>
                              <value>1</value>
                              <range>100</range>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                            <border>5</border>
                            <object class="wxSlider" name="test_slider">
                              <size>100,-1</size>
                              <style>wxSL_HORIZONTAL</style>
                              <value>0</value>
                              <min>0</min>
                              <max>100</max>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
                            <border>5</border>
                            <object class="wxScrollBar" name="test_scrollbar">
                              <size>100,-1</size>
                              <style>wxSB_HORIZONTAL</style>
                              <value>0</value>
                              <range>100</range>
                              <thumbsize>1</thumbsize>
                              <pagesize>1</pagesize>
                            </object>
                          </object>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxGROW|wxALL</flag>
                        <border>5</border>
                        <object class="wxCheckList" name="test_checklist">
                          <content>
                            <item>Eyes</item>
                            <item>Ears</item>
                            <item>Nose</item>
                            <item>Mouth</item>
                          </content>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxGROW|wxALL</flag>
                        <border>5</border>
                        <object class="wxTreeCtrl" name="test_tree">
                          <size>100,100</size>
                          <style>wxTR_SINGLE</style>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxGROW|wxGROW|wxALL</flag>
                        <border>5</border>
                        <object class="wxGenericDirCtrl" name="test_dir">
                          <size>100,100</size>
                          <style></style>
                          <defaultfolder></defaultfolder>
                          <filter>All files (*.*)|*.*</filter>
                          <defaultfilter>0</defaultfilter>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            
          </object>
        </object>
      </object>
    </object>
  </object>
</resource>
